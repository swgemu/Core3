	public void setMovementState(int state) {
		synchronized (targetMutex) {
			int oldState = movementState;

			if (state != PATROLLING && state != WATCHING)
				clearPatrolPoints();

			movementState = state;

			if (oldState == LEASHING || state == LEASHING)
				broadcastPvpStatusBitmask();
		}
	}

