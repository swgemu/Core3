	public ShipObject() {
		Logger.setLoggingName("ShipObject");

		super.objectVisible = 1;

		super.closeobjects = new CloseObjectsVector();
		super.closeobjects.setNoDuplicateInsertPlan();
		controlDeviceID = 0;
		hyperspacing = false;
		hasWings = false;

		chassisLevel = 0;
		totalCellNumber = 0;

		playersOnBoard.setNoDuplicateInsertPlan();

		boundingRadius = 1.f;
		currentSpeed = 0.f;
		speedRotationFactorMax = 1.f;
		speedRotationFactorMin = 1.f;
		speedRotationFactorOptimal = 0.5f;

		chassisSpeed.set(1.f);
	}

